                }
            });
        }
    };

    function updateRTU2warnings() {
        var PriorityCol = -1;
        PriorityCol = $('#RTU2WarningWindow th[aria-label*="Priority"]').index();
        if (PriorityCol > -1) {
            $('#RTU2WarningWindow tr').not('thead tr').each(function (idx, row) {
                var cellData = $('#RTU2WarningWindow').find('td').eq(PriorityCol)[0].innerText;
                $('#RTU2WarningWindow th[aria-label*="Priority"]').hide();
                $(this).find('td').eq(PriorityCol).hide();
                switch (cellData) {
                    case '0':
                        $(this).addClass("WarningRow");
                        break;
                    case '100':
                        $(this).addClass("AlarmRow").addClass("HideAlarm");
                        break;
                    default:
                        break;
                }
            });
        }
    };

    function opc_rtu_callback(data) {
        Update_Tagged_Controls();
    }

    function createGauges() {
        $("#gaugeAMBTEMP").kendoRadialGauge({
            pointer: [{
                value: 0,
                color: "#009900",
                cap: { size: 0.15 }
            }],
            scale: {
                min: -20,
                minorUnit: 5,
                startAngle: -30,
                endAngle: 210,
                max: 120,
                labels: {
                    position: "outside"
                },
                ranges: [
                    {
                        from: -20,
                        to: 0,
                        color: "#c20000"
                    }, {
                        from: 0,
                        to: 40,
                        color: "#ffd400"
                    }, {
                        from: 40,
                        to: 70,
                        color: "#00c000"
                    }, {
                        from: 70,
                        to: 100,
                        color: "#ffd400"
                    }, {
                        from: 100,
                        to: 120,
                        color: "#c20000"
                    }
                ]
            }
        })
        $("#gaugeDISCHTEMP").kendoRadialGauge({
            pointer: [{
                value: 0,
                color: "#009900",
                cap: { size: 0.15 }
            }],
            scale: {
                min: -20,
                minorUnit: 5,
                startAngle: -30,
                endAngle: 210,
                max: 120,
                labels: {
