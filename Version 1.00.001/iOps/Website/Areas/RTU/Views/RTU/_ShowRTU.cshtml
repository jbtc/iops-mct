                },
                ranges: [
                    {
                        from: -20,
                        to: 0,
                        color: "#c20000"
                    }, {
                        from: 0,
                        to: 40,
                        color: "#ffd400"
                    }, {
                        from: 40,
                        to: 70,
                        color: "#00c000"
                    }, {
                        from: 70,
                        to: 100,
                        color: "#ffd400"
                    }, {
                        from: 100,
                        to: 120,
                        color: "#c20000"
                    }
                ]
            }
        })
        $("#gaugeCABINTEMP").kendoRadialGauge({
            pointer: [{
                value: 0,
                color: "#009900",
                cap: { size: 0.15 }
            }],
            scale: {
                min: -20,
                minorUnit: 5,
                startAngle: -30,
                endAngle: 210,
                max: 120,
                labels: {
                    position: "outside"
                },
                ranges: [
                    {
                        from: -20,
                        to: 0,
                        color: "#c20000"
                    }, {
                        from: 0,
                        to: 40,
                        color: "#ffd400"
                    }, {
                        from: 40,
