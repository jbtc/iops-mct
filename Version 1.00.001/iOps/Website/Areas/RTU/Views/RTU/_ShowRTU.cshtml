    function CloseKendoRTU() {

        if ($('#RTUWindow').is(':visible')) {
            $('#RTUWindow').data('kendoWindow').destroy();
            return false;
        }
    }

    function CloseRTUAlarmWindow() {
        $('#RTUAlarmWindow').data('kendoWindow').destroy();
        return false;
    }

    function CloseRTUWarningWindow() {
        $('#RTUWarningWindow').data('kendoWindow').destroy();
        return false;
    }

    function get_value(tag) {
        return OPC.get_value(tag);
        //return OPC.get_value(OPC_config.tag_prefix + tag);
    }
    function get_bool(tag) {
        var b = OPC.get_value(tag);
        //var b = OPC.get_value(OPC_config.tag_prefix + tag);
        if (b && b == "True") return true;
        if (b && b == "False") return false;
        return null;
    }

    function Update_Tagged_Controls() {
        var pre = OPC_config.tag_prefix;
        var gauge = $("#gaugeAMBTEMP").data("kendoRadialGauge");
        gauge.pointers[0].value(parseInt(get_value(pre + "TEMPAMB.Value")));

        gauge = $("#gaugeDISCHTEMP").data("kendoRadialGauge");
        gauge.pointers[0].value(parseInt(get_value(pre + "TEMPDISCH.Value")));

        gauge = $("#gaugeCABINTEMP").data("kendoRadialGauge");
        gauge.pointers[0].value(parseInt(get_value(pre + "TEMRTUBIN.Value")));
    }

    function updateRTU1Alarms() {
        var PriorityCol = -1;
        PriorityCol = $('#RTU1AlarmWindow th[aria-label*="Priority"]').index();
        if (PriorityCol > -1) {
            $('#RTU1AlarmWindow tr').not('thead tr').each(function (idx, row) {
                var cellData = $('#RTU1AlarmWindow').find('td').eq(PriorityCol)[0].innerText;
                $('#RTU1AlarmWindow th[aria-label*="Priority"]').hide();
                $(this).find('td').eq(PriorityCol).hide();
                switch (cellData) {
                    case '0':
                        $(this).addClass("WarningRow").addClass("HideAlarm");
                        break;
                    case '100':
                        $(this).addClass("AlarmRow");
                        break;
                    default:
                        break;
                }
            });
        }
    };

