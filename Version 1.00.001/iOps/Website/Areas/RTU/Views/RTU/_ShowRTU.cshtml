            $('#RTUAlarmWindow').data('kendoWindow').open();
            $('#RTUAlarmWindow').closest(".k-window").css({ top: 90, left: 250 });
        })

        var rtuWarningWindow = $('#RTUWarningWindow');
        if (!rtuWarningWindow.data('kendoWindow')) {
